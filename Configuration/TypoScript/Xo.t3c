plugin.tx_xo {
	view {
		# cat=plugin.xo/file; type=string; label=Path to template root (FE)
		templateRootPath = EXT:xo/Resources/Private/Templates/

		# cat=plugin.xo/file; type=string; label=Path to template partials (FE)
		partialRootPath = EXT:xo/Resources/Private/Partials/

		# cat=plugin.xo/file; type=string; label=Path to template layouts (FE)
		layoutRootPath = EXT:xo/Resources/Private/Layouts/
	}

	persistence {
		# cat=plugin.xo//a; type=string; label=Default storage PID
		storagePid = 3
	}

	settings {
		svg {
			# cat=plugin.xo/settings; type=string; label=Default path for svg files
			path = fileadmin/Resources/Public/Svg/
		}
	}
}

// Default paths for resources.
folders {
	private = typo3conf/ext/xo/Resources/Private
	public = typo3conf/ext/xo/Resources/Public

	images = fileadmin/Resources/Public/Images
	css = fileadmin/Resources/Public/Css
	js = fileadmin/Resources/Public/Js
}

url {
	# cat=url; type=string; label=Assets absolute URL
	assets = /assets/
}

meta {
	# cat=meta; type=string; label=Keywords: Enter a comma separated list of keywords.
	keywords =

	# cat=meta; type=string; label=Description: Write a short abstract for your website.
	description =

	# cat=meta; type=string; label=Author: Enter name of author.
	author =

	# cat=meta; type=string; label=Robots: Use for instance these codes: Index all pages: "all".  Index no pages: "none". Only this page: "index,nofollow".  Only subpages: "noindex,follow"
	robots = NOODP

	# cat=meta; type=string; label=Viewport
	viewport = width=device-width, initial-scale=1
}

config {
	# cat=config; type=boolean; label=Debug: Activate the debug mode.
	debug = 0

	# cat=config; type=boolean; label=Cache: Deactivate the cache.
	no_cache = 1

	# cat=config; type=boolean; label=AdminPanel: Activate the admin panel.
	adminPanel = 0

	# cat=config; type=string; label=AbsRefPrefix: Set the AbsRefPrefix.
	absRefPrefix = /
}

navigation {
	# cat=navigation; type=int+; label=Main-Navigation folder page: Page id of the folder which contains the main-navigation.
	main = 4

	# cat=navigation; type=int+; label=Footer-Navigation folder page: Page id of the folder which contains the footer-navigation.
	footer = 5

	# cat=navigation; type=int+; label=Meta-Navigation folder page: Page id of the folder which contains the meta-navigation.
	meta = 54

	# cat=navigation; type=string; label=List of Language IDs
	language = 0, 1
}

pages {
	# cat=pages; type=int+; label=Page id of home
	home = 1

	# cat=pages; type=int+; label=Page id for search results
	search = 38

	# cat=pages; type=int+; label=Page id for main contact
	contact = 111
}

googleAnalytics {
	# cat=googleAnalytics; type=boolean; label=Activate Google Analytics Snippet
	enabled = 0

	# cat=googleAnalytics; type=string; label=Tracking-ID
	trackingId = UA-123-4
}

googleMaps {
	# cat=googleMaps; type=string; label=Uri for Google Maps API
	uri = https://maps.googleapis.com/maps/api/js?key=

	# cat=googleMaps; type=string; label=Google Maps API Key
	apiKey = AIzaSyCOkLaiTykKBxj4oCyaRzuYZp0k2XCBLtc
}

addresses {

	# cat=addresses; type=string; label=ID of provider data record (tt_address)
	provider = 14
}

styles.content.textmedia {
	maxW = 1200
	maxWInText = 800
}

moduleProcessor {

	# cat=moduleProcessor; type=boolean; label=Activate module css integration
	cssModuleEnabled = 1

	# cat=moduleProcessor; type=int+; label=Number of modules which CSS would be inline
	cssModuleInlineLimit = 2

	# cat=moduleProcessor; type=boolean; label=Use, if exists, a minified version (.min.css) in production environment
	cssUseMinifyOnProduction = 1

	# cat=moduleProcessor; type=boolean; label=Activate module js integration
	jsModuleEnabled = 1

	# cat=moduleProcessor; type=int+; label=Number of modules which JS would be inline
	jsModuleInlineLimit = 0

	# cat=moduleProcessor; type=boolean; label=Use, if exists, a minified version (.min.css) in production environment
	jsUseMinifyOnProduction = 1
}

flash {

	# cat=flash; type=int+; label=ColPos for flash content elements
	colPos = 326022
}